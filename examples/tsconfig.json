{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "pretty": true,
        "removeComments": true,
        "outDir": "./"
    }, "include": [
        "typings/**/*.d.ts",
        "*.ts"
    ], "exclude": [
        "node_modules"
    ]
}